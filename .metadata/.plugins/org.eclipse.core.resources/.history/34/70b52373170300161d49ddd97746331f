class Testing(unittest.TestCase):
    def positiveTesting(self):
        #addObj = Addition()
        self.assertEquals(5+4, 9, "Test Passed !")
    
    def negativeTesting(self):
        #addObj = Addition()
        self.assertEquals(2+4, 9, "Test Failed !")
        
    
